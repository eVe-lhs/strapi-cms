{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "Project",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Type": {
      "type": "enumeration",
      "enum": [
        "Web_Development",
        "Programming",
        "Others"
      ],
      "required": true,
      "default": "Others"
    },
    "Cover": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "GitLink": {
      "type": "string",
      "required": true
    },
    "tags": {
      "via": "tag",
      "collection": "tag",
      "dominant": true
    }
  }
}
